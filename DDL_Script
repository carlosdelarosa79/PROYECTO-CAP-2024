
CREATE TABLE proyecto_personal_Products (
  ID NVARCHAR(36) NOT NULL,
  createdAt TIMESTAMP_TEXT,
  createdBy NVARCHAR(255),
  modifiedAt TIMESTAMP_TEXT,
  modifiedBy NVARCHAR(255),
  Name NVARCHAR(255),
  Description NVARCHAR(255),
  ImageUrl NVARCHAR(255),
  ReleaseDate DATETIME_TEXT DEFAULT CURRENT_TIMESTAMP,
  DiscontinuedDate DATETIME_TEXT,
  Price DECIMAL(16, 2),
  Height DECIMAL(16, 2),
  Width DECIMAL(16, 2),
  Depth DECIMAL(16, 2),
  Quantity DECIMAL(16, 2),
  Supplier_ID NVARCHAR(36),
  UnitOfMeasure_ID NVARCHAR(36),
  Currency_ID NVARCHAR(36),
  DimensionsUnit_ID NVARCHAR(36),
  Category_ID NVARCHAR(36),
  PRIMARY KEY(ID)
);

CREATE TABLE proyecto_personal_Orders (
  ID NVARCHAR(36) NOT NULL,
  Date DATE_TEXT,
  Customer NVARCHAR(255),
  PRIMARY KEY(ID)
);

CREATE TABLE proyecto_personal_OrderItems (
  ID NVARCHAR(36) NOT NULL,
  Order_ID NVARCHAR(36),
  Product_ID NVARCHAR(36),
  Quantity INTEGER,
  PRIMARY KEY(ID)
);

CREATE TABLE proyecto_personal_Suppliers (
  ID NVARCHAR(36) NOT NULL,
  createdAt TIMESTAMP_TEXT,
  createdBy NVARCHAR(255),
  modifiedAt TIMESTAMP_TEXT,
  modifiedBy NVARCHAR(255),
  Name NVARCHAR(255),
  Address_Street NVARCHAR(255),
  Address_City NVARCHAR(255),
  Address_State NVARCHAR(2),
  Address_PostalCode NVARCHAR(5),
  Address_Country NVARCHAR(3),
  Email NVARCHAR(255),
  Phone NVARCHAR(255),
  Fax NVARCHAR(255),
  PRIMARY KEY(ID)
);

CREATE TABLE proyecto_personal_Categories (
  ID NVARCHAR(36) NOT NULL,
  Name NVARCHAR(255),
  PRIMARY KEY(ID)
);

CREATE TABLE proyecto_personal_StockAvailability (
  ID NVARCHAR(36) NOT NULL,
  Description NVARCHAR(255),
  Product_ID NVARCHAR(36),
  PRIMARY KEY(ID)
);

CREATE TABLE proyecto_personal_Currencies (
  ID NVARCHAR(36) NOT NULL,
  Description NVARCHAR(255),
  PRIMARY KEY(ID)
);

CREATE TABLE proyecto_personal_UnitOfMeasures (
  ID NVARCHAR(36) NOT NULL,
  Description NVARCHAR(255),
  PRIMARY KEY(ID)
);

CREATE TABLE proyecto_personal_DimensionsUnits (
  ID NVARCHAR(36) NOT NULL,
  Description NVARCHAR(255),
  PRIMARY KEY(ID)
);

CREATE TABLE proyecto_personal_Months (
  ID NVARCHAR(36) NOT NULL,
  Description NVARCHAR(255),
  ShortDescription NVARCHAR(255),
  PRIMARY KEY(ID)
);

CREATE TABLE proyecto_personal_ProductReview (
  ID NVARCHAR(36) NOT NULL,
  createdAt DATETIME_TEXT,
  createdBy NVARCHAR(255),
  modifiedAt NVARCHAR(255),
  modifiedBy NVARCHAR(255),
  Name NVARCHAR(255),
  Rating INTEGER,
  Comment NVARCHAR(255),
  Product_ID NVARCHAR(36),
  PRIMARY KEY(ID)
);

CREATE TABLE proyecto_personal_SalesData (
  ID NVARCHAR(36) NOT NULL,
  createdAt TIMESTAMP_TEXT,
  createdBy NVARCHAR(255),
  modifiedAt TIMESTAMP_TEXT,
  modifiedBy NVARCHAR(255),
  DeliveryDate DATETIME_TEXT,
  Revenue DECIMAL(16, 2),
  Product_ID NVARCHAR(36),
  Currency_ID NVARCHAR(36),
  DeliveryMonth_ID NVARCHAR(36),
  PRIMARY KEY(ID)
);

CREATE TABLE proyecto_personal_Products_texts (
  locale NVARCHAR(14) NOT NULL,
  ID NVARCHAR(36) NOT NULL,
  Name NVARCHAR(255),
  Description NVARCHAR(255),
  PRIMARY KEY(locale, ID)
);

CREATE TABLE proyecto_personal_Categories_texts (
  locale NVARCHAR(14) NOT NULL,
  ID NVARCHAR(36) NOT NULL,
  Name NVARCHAR(255),
  PRIMARY KEY(locale, ID)
);

CREATE TABLE proyecto_personal_StockAvailability_texts (
  locale NVARCHAR(14) NOT NULL,
  ID NVARCHAR(36) NOT NULL,
  Description NVARCHAR(255),
  PRIMARY KEY(locale, ID)
);

CREATE TABLE proyecto_personal_Currencies_texts (
  locale NVARCHAR(14) NOT NULL,
  ID NVARCHAR(36) NOT NULL,
  Description NVARCHAR(255),
  PRIMARY KEY(locale, ID)
);

CREATE TABLE proyecto_personal_UnitOfMeasures_texts (
  locale NVARCHAR(14) NOT NULL,
  ID NVARCHAR(36) NOT NULL,
  Description NVARCHAR(255),
  PRIMARY KEY(locale, ID)
);

CREATE TABLE proyecto_personal_DimensionsUnits_texts (
  locale NVARCHAR(14) NOT NULL,
  ID NVARCHAR(36) NOT NULL,
  Description NVARCHAR(255),
  PRIMARY KEY(locale, ID)
);

CREATE TABLE proyecto_personal_Months_texts (
  locale NVARCHAR(14) NOT NULL,
  ID NVARCHAR(36) NOT NULL,
  Description NVARCHAR(255),
  ShortDescription NVARCHAR(255),
  PRIMARY KEY(locale, ID)
);

CREATE TABLE proyecto_personal_Suppliers_texts (
  locale NVARCHAR(14) NOT NULL,
  ID NVARCHAR(36) NOT NULL,
  Name NVARCHAR(255),
  PRIMARY KEY(locale, ID)
);

CREATE VIEW localized_proyecto_personal_Products AS SELECT
  L_0.ID,
  L_0.createdAt,
  L_0.createdBy,
  L_0.modifiedAt,
  L_0.modifiedBy,
  coalesce(localized_1.Name, L_0.Name) AS Name,
  coalesce(localized_1.Description, L_0.Description) AS Description,
  L_0.ImageUrl,
  L_0.ReleaseDate,
  L_0.DiscontinuedDate,
  L_0.Price,
  L_0.Height,
  L_0.Width,
  L_0.Depth,
  L_0.Quantity,
  L_0.Supplier_ID,
  L_0.UnitOfMeasure_ID,
  L_0.Currency_ID,
  L_0.DimensionsUnit_ID,
  L_0.Category_ID
FROM (proyecto_personal_Products AS L_0 LEFT JOIN proyecto_personal_Products_texts AS localized_1 ON localized_1.ID = L_0.ID AND localized_1.locale = 'en');

CREATE VIEW localized_proyecto_personal_Suppliers AS SELECT
  L_0.ID,
  L_0.createdAt,
  L_0.createdBy,
  L_0.modifiedAt,
  L_0.modifiedBy,
  coalesce(localized_1.Name, L_0.Name) AS Name,
  L_0.Address_Street,
  L_0.Address_City,
  L_0.Address_State,
  L_0.Address_PostalCode,
  L_0.Address_Country,
  L_0.Email,
  L_0.Phone,
  L_0.Fax
FROM (proyecto_personal_Suppliers AS L_0 LEFT JOIN proyecto_personal_Suppliers_texts AS localized_1 ON localized_1.ID = L_0.ID AND localized_1.locale = 'en');

CREATE VIEW localized_proyecto_personal_Categories AS SELECT
  L_0.ID,
  coalesce(localized_1.Name, L_0.Name) AS Name
FROM (proyecto_personal_Categories AS L_0 LEFT JOIN proyecto_personal_Categories_texts AS localized_1 ON localized_1.ID = L_0.ID AND localized_1.locale = 'en');

CREATE VIEW localized_proyecto_personal_StockAvailability AS SELECT
  L_0.ID,
  coalesce(localized_1.Description, L_0.Description) AS Description,
  L_0.Product_ID
FROM (proyecto_personal_StockAvailability AS L_0 LEFT JOIN proyecto_personal_StockAvailability_texts AS localized_1 ON localized_1.ID = L_0.ID AND localized_1.locale = 'en');

CREATE VIEW localized_proyecto_personal_Currencies AS SELECT
  L_0.ID,
  coalesce(localized_1.Description, L_0.Description) AS Description
FROM (proyecto_personal_Currencies AS L_0 LEFT JOIN proyecto_personal_Currencies_texts AS localized_1 ON localized_1.ID = L_0.ID AND localized_1.locale = 'en');

CREATE VIEW localized_proyecto_personal_UnitOfMeasures AS SELECT
  L_0.ID,
  coalesce(localized_1.Description, L_0.Description) AS Description
FROM (proyecto_personal_UnitOfMeasures AS L_0 LEFT JOIN proyecto_personal_UnitOfMeasures_texts AS localized_1 ON localized_1.ID = L_0.ID AND localized_1.locale = 'en');

CREATE VIEW localized_proyecto_personal_DimensionsUnits AS SELECT
  L_0.ID,
  coalesce(localized_1.Description, L_0.Description) AS Description
FROM (proyecto_personal_DimensionsUnits AS L_0 LEFT JOIN proyecto_personal_DimensionsUnits_texts AS localized_1 ON localized_1.ID = L_0.ID AND localized_1.locale = 'en');

CREATE VIEW localized_proyecto_personal_Months AS SELECT
  L_0.ID,
  coalesce(localized_1.Description, L_0.Description) AS Description,
  coalesce(localized_1.ShortDescription, L_0.ShortDescription) AS ShortDescription
FROM (proyecto_personal_Months AS L_0 LEFT JOIN proyecto_personal_Months_texts AS localized_1 ON localized_1.ID = L_0.ID AND localized_1.locale = 'en');

CREATE VIEW localized_proyecto_personal_OrderItems AS SELECT
  L.ID,
  L.Order_ID,
  L.Product_ID,
  L.Quantity
FROM proyecto_personal_OrderItems AS L;

CREATE VIEW localized_proyecto_personal_ProductReview AS SELECT
  L.ID,
  L.createdAt,
  L.createdBy,
  L.modifiedAt,
  L.modifiedBy,
  L.Name,
  L.Rating,
  L.Comment,
  L.Product_ID
FROM proyecto_personal_ProductReview AS L;

CREATE VIEW localized_proyecto_personal_SalesData AS SELECT
  L.ID,
  L.createdAt,
  L.createdBy,
  L.modifiedAt,
  L.modifiedBy,
  L.DeliveryDate,
  L.Revenue,
  L.Product_ID,
  L.Currency_ID,
  L.DeliveryMonth_ID
FROM proyecto_personal_SalesData AS L;

CREATE VIEW localized_proyecto_personal_Orders AS SELECT
  L.ID,
  L.Date,
  L.Customer
FROM proyecto_personal_Orders AS L;

